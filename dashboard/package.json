  "description": "SOLARIA C-Suite Dashboard - SupervisiÃ³n de proyectos gestionados por agentes IA",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:ts": "node dist/server.js",
    "dev": "tsx watch server.ts",
    "dev:js": "nodemon server.js",
    "test": "jest",
    "build": "npm run build-css && npm run build-js",
    "build:ts": "tsc",
    "build-css": "tailwindcss -i ./src/styles/input.css -o ./public/styles.css --minify",
    "build-js": "webpack --mode production",
    "setup": "node scripts/setup-database.js",
    "seed": "node scripts/seed-data.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json}\"",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit introspect",
    "db:studio": "drizzle-kit studio"
  },
  "keywords": [
    "dashboard",
    "c-suite",
    "ai-agents",
    "project-management",
    "solaria"
  ],
  "author": "SOLARIA AGENCY",
  "license": "PROPRIETARY",
